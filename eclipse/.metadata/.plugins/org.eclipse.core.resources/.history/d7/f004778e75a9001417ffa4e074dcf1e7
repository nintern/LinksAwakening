package com.nintern.linksawakening.init;

import com.nintern.linksawakening.block.BlockLinksAwakening;
import com.nintern.linksawakening.block.BlockTable;

import cpw.mods.fml.common.registry.GameRegistry;

@GameRegistry.ObjectHolder
public class ModBlocks
{
	public static final BlockLinksAwakening table = new BlockTable();
	
	public static void init()
	{
		GameRegistry.registerBlock(table, "table");
	}
}
